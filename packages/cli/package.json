{
  "name": "@faasjs/cli",
  "version": "7.0.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "faas": "index.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "homepage": "https://faasjs.com/doc/cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faasjs/faasjs.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/faasjs/faasjs/issues"
  },
  "funding": "https://github.com/sponsors/faasjs",
  "scripts": {
    "build": "tsup-node src/index.ts --config ../../tsup.config.ts"
  },
  "files": [
    "dist",
    "index.js",
    "devServer.ts"
  ],
  "dependencies": {
    "@faasjs/test": ">=7.0.0",
    "commander": "*",
    "enquirer": "*"
  },
  "peerDependencies": {
    "@faasjs/func": ">=7.0.0",
    "@faasjs/server": ">=7.0.0"
  },
  "devDependencies": {
    "@faasjs/func": ">=7.0.0",
    "@faasjs/server": ">=7.0.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.0.0"
  }
}
