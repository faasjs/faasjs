{
  "name": "@faasjs/node-utils",
  "version": "8.0.0-beta.12",
  "homepage": "https://faasjs.com/doc/node-utils",
  "bugs": {
    "url": "https://github.com/faasjs/faasjs/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faasjs/faasjs.git",
    "directory": "packages/node-utils"
  },
  "funding": "https://github.com/sponsors/faasjs",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./register-hooks": {
      "types": "./dist/register_hooks.d.ts",
      "import": "./dist/register_hooks.mjs",
      "require": "./dist/register_hooks.cjs"
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts src/register_hooks.ts --config ../../tsdown.config.ts"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=11.0.0"
  }
}
