{
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    ".temp": true,
    "**/.git": true,
    "**/node_modules": true,
  },
  "eslint.packageManager": "npm",
  "cSpell.words": [
    "Antd",
    "anticon",
    "builtins",
    "colorfy",
    "Faas",
    "faasjs",
    "onwarn",
    "querystringify",
    "RUNENV",
    "SECRETID",
    "SECRETKEY",
    "SESSIONTOKEN",
    "Tencentcloud",
    "tencentcloudapi",
    "typedoc",
    "Useify",
    "vuepress",
    "zfben"
  ]
}
