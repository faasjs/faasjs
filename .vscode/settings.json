{
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll.oxc": "explicit"
  },
  "oxc.fmt.configPath": "packages/dev/configs/oxfmt.base.json",
  "editor.defaultFormatter": "oxc.oxc-vscode",
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "cSpell.words": [
    "Antd",
    "anticon",
    "builtins",
    "colorfy",
    "Faas",
    "faasjs",
    "Millis",
    "onwarn",
    "oxc",
    "oxfmt",
    "oxlint",
    "querystringify",
    "RUNENV",
    "SECRETID",
    "SECRETKEY",
    "SESSIONTOKEN",
    "testid",
    "typedoc",
    "Useify",
    "vitest",
    "vuepress",
    "zfben"
  ]
}
